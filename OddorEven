public class AddOrEven {

public static void main(String[] args) {
	
	int n=12;
	System.out.println("Add Numbers   "+"Even numbers");
	for (int i = 1; i <=n; i++) {
		if (i%2==0) {
			System.out.println(i);
		}
		else {
			if (i%2!=0) {
				System.out.print(i+"               ");
			}
		}
	}
}
	
	
}
